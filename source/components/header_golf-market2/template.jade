    section#predzakaz(class='bj-page-header__dropdown')
      .i-relative
        article
          .container-fluid
            div(class='bj-page-header__dropdown-hr')
            div(class='bj-page-header__dropdown-container')
              div(class='bj-page-header__dropdown-links')
                a(href='') Игровые мячи
                a(href='') Тренировочные
                a(href='') Дальномеры
              div(class='bj-page-header__dropdown-banner')
                img(src='/upload/stories3.jpg' alt='')
    section#userDropdown(class='bj-page-header__user-dropdown')
      .i-relative
        article
          .container-fluid
            a(href='') Войти
            a(href='') Регистрация
            hr
            a(href='') tatiana
            a(href='') Мои заказы
            a(href='') Выйти
    #myModal.modal.fade(tabindex='-1' role='dialog' aria-labelledby='myModalLabel')
      .modal-dialog(role='document')
        .modal-content
          .modal-header
            button.close(type='button' data-dismiss='modal' aria-label='Close')
            h2#myModalLabel.modal-title.text-center
              | &Ncy;&acy;&shcy; &tcy;&iecy;&lcy;&iecy;&fcy;&ocy;&ncy;
          .modal-body.text-center
            .bj-text-x-large 8 (495) 212 85 06
            .small &Pcy;&ncy;-&Vcy;&scy; 10-19
          .modal-footer.text-center(style='display: none')
            button.btn.btn-default(type='button')
              | &Zcy;&acy;&kcy;&acy;&zcy;&acy;&tcy;&softcy;
              | &zcy;&vcy;&ocy;&ncy;&ocy;&kcy;
